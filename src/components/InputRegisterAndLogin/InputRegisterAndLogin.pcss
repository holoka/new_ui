#custom-input-register-login {
	.form-input {
		background-color: #6c757d !important;
		border: 1px solid rgba(47, 182, 126, 0.33);
		border-radius: 5px;
		box-sizing: border-box;
		color: #fff !important;
		height: 40px;
	}

	.toggle-input {
		border-radius: 5px;
		height: 20px;
		margin-left: 10px;
		width: 20px;
	}
}
