#ieo-detail-screen {
	margin-bottom: 80px;
	padding-left: 85px;
	padding-right: 85px;

	.ieo-title {
		color: #fff;
		font-size: 35px;
		margin-bottom: 50px;
		margin-top: 32px;
	}

	#ioe-detail-screen__return-list {
		background: rgba(132, 142, 156, 0.35);
		border: 1px solid #a3a3a3;
		border-radius: 5px;
		box-sizing: border-box;
		color: #fff;
		font-size: 16px;
		height: 45px;
		margin-bottom: 55px;
		width: 200px;
	}

	.buy-history-view {
		background: #434a56;
		margin: auto;
		margin-bottom: 30px;
		padding-bottom: 50px;
		padding-top: 50px;
	}
}
