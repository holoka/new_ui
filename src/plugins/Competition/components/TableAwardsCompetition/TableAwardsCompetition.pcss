#table-award-competition {
	table {
		color: #fff;
		box-shadow: 0px 70px 100px rgba(0, 0, 0, 0.08);
		border-radius: 5px;
		thead {
			background: #4e5363;
			font-size: 18px;
			border-radius: 3px 3px 0px 0px;
			tr {
				th {
					font-weight: normal;
				}
			}
		}
		tbody {
			background: #313445;
			box-shadow: 0px 70px 100px rgba(0, 0, 0, 0.08);
			tr {
				font-size: 15px;
			}
		}
	}
}
